{
  "name": "All Mangas Reader",
  "description": "__MSG_description__",
  "author": "",
  "version": "2.0.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "All Mangas Reader",
    "default_popup": "pages/popup/popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "lib/jquery.min.js",
      "background/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "*://*.allmangasreader.com/*"
      ],
      "js": [
        "content/testcontent.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "unlimitedStorage",
    "contextMenus",
    "notifications",
    "downloads"
  ],
  "content_security_policy": "script-src 'self' https://localhost:8080/ https://ssl.google-analytics.com       https://community.allmangasreader.com/ https://raw.github.com/ ; object-src 'self'",
  "web_accessible_resources" : [
    "icons/icon_32.png",
    "icons/down.png",
    "icons/bookmark.png",
    "icons/read_stop.png",
    "icons/lefttoright.png",
    "icons/stop.gif",
    "icons/loading.gif",
    "icons/cancel.png",
    "icons/ontop.png",
    "icons/righttoleft.png",
    "icons/load16.gif",
    "icons/add.png",
    "icons/bookmarkred.png",
    "icons/warn.png",
    "icons/read_play.png",
    "icons/amrlittle.png",
    "icons/dev.png",
    "icons/ltload.gif",
    "icons/imgerror.png",
    "icons/load10.gif",
    "icons/find.png"
  ]
}